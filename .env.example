# OpenAI API Key (or other LLM provider)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Custom runtime URL (leave undefined for default)
# NEXT_PUBLIC_COPILOTKIT_RUNTIME_URL=

# Cal.com API Configuration
# Get your API key from https://app.cal.com/settings/developer/api-keys
CAL_COM_API_KEY=your_cal_com_api_key_here
# Event type ID for consultations (required)
# Find your event type ID by visiting: https://api.cal.com/v2/event-types
CAL_COM_EVENT_TYPE_ID=4097546

# Supabase Configuration
# Get your credentials from https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
